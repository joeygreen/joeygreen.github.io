<div class="search">
	<input placeholder="Search {{station_name}}" type="text" name="search" value="{{search_value}}" on-keyup="search:{{search_value}}">
</div>
<div id="search-results">
	{{message}}
	<table class="table-link clean">
	{{#tracks}}
		<tr on-click="request:{{track_id}}" class="link" >
			<td>{{name}}</td>
			<td style="width: 30px;" class="time r">{{seconds_h}}</td>
		</tr>
	{{/tracks}}
	{{#if more}}
		<a class="link">More</a>
	{{/if}}
	</table>
</div>
